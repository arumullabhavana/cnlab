outline-style:solid;
height:170px;
}
.static{
position:static;
margin:10px;
height:50px;
padding:10px;
background-color:orange;
width:50px;
border:1px solid black;
}
.relative{
position:relative;
margin:10px;
height:50px;
padding:10px;
background-color:blue;
width:50px;
border:1px solid black;
}
.absolute{
position:absolute;
padding:20px;
margin:10px;
top:100px;
right:50px;
background-color:green;
border:1px solid black;
}
.fixed{
position:fixed;
height:50px;
width:50px;
bottom:20px;
left:50px;
background-color:pink;
border:1px solid black;
}
.sticky{
position:sticky;
margin:40px;
top:0px;
height:50px;
width:50px;
padding:10px;
background-color:yellow;
border:1px solid black;
}
